<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2015/06/nuspec.xsd">
  <metadata>
    <id>keeper-password-manager</id>
    <version>14.8.0</version>
    <packageSourceUrl>https://github.com/steviecoaster/PSSaturday</packageSourceUrl>
    <owners>steviecoaster</owners>
    <title>keeper-password-manager (Install)</title>
    <authors>Keeper Security, Inc</authors>
    <projectUrl>https://github.com/steviecoaster/choco-packages</projectUrl>
    <copyright>Year Software Vendor</copyright>
    <licenseUrl>https://keepersecurity.com/termsofuse.html</licenseUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <docsUrl>https://docs.keeper.io/user-guides/web-vault</docsUrl>
    <tags>keeper-password-manager password passwordmanager secret</tags>
    <summary>Installs the 32-bit or 64-bit version of Keeper desktop</summary>
    <description>With Keeper, you can securely store and autofill unlimited passwords with our free password manager. You’ll never have to remember or worry about passwords again.</description>
    <dependencies>
      <dependency id="chocolatey-core.extension" />
    </dependencies>
  </metadata>
  <files>
    <!-- this section controls what actually gets packaged into the Chocolatey package -->
    <file src="tools\**" target="tools" />
    <!--Building from Linux? You may need this instead: <file src="tools/**" target="tools" />-->
  </files>
</package>
